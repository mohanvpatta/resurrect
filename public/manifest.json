{
  "manifest_version": 2,
  "name": "Resurrect",
  "version": "1.0.0",
  "description": "Rearrange your tabs by URL",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "128": "128.png"
    }
  },
  "permissions": ["tabs"]
}
